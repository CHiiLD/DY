<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:DY.WPF"
	mc:Ignorable="d"
	x:Class="DY.WPF.CommDeviceAddition"
	x:Name="UserControl"
	d:DesignWidth="240" d:DesignHeight="300">

	<Grid x:Name="LayoutRoot">
		<StackPanel Orientation="Vertical">
			<local:ComboBoxWithBar Title="Communication Device" Height="300"/>
			<local:ComboBoxWithBar Title="Communication Type" Height="300"/>
		</StackPanel>
	</Grid>

</UserControl>